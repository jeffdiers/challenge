  "main": "index.js",
  "scripts": {
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist && mkdir dist",
    "cp-sw": "cp ./framework/mock-server/mockServiceWorker.js dist/",
    "build": "npm run type-check && npm run clean && npm run cp-sw && npm run build:server && npm run build:client",
    "build:server": "node esbuild.server.js",
    "build:client": "node esbuild.client.js",
    "start": "npm run build && node dist/server.js",
    "dev": "ts-node framework/server/index.ts",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "esbuild": "^0.21.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.2"
  },
  "dependencies": {
    "fastify": "^4.27.0",
    "msw": "^2.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "valibot": "^0.30.0"
  },
  "msw": {
